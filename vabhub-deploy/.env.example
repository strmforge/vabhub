# VabHub 部署环境变量配置

# 数据库配置
POSTGRES_DB=vabhub
POSTGRES_USER=vabhub
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432

# Redis配置
REDIS_PASSWORD=your_redis_password_here
REDIS_PORT=6379

# 应用端口配置
CORE_PORT=8080
WEB_PORT=8090

# 环境配置
VABHUB_ENV=production
TZ=Asia/Shanghai

# 监控配置
GRAFANA_PASSWORD=admin

# 安全配置
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# 外部服务配置
MEDIA_SERVER_URL=http://your-media-server:8096
CLOUD_STORAGE_ENDPOINT=https://your-cloud-storage.com

# 插件配置
PLUGIN_DIR=/app/plugins
LOG_LEVEL=INFO